{
    "manifest_version": 2,
    "name": "Sponsored post remover",
    "description": "Removes sponsored posts from fb",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://www.facebook.com/*"],
            "js": ["scripts/remover.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}
